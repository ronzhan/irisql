  "bugs": {
    "url": "https://github.com/oslabs-beta/irisql/issues"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.(jpg|jpeg|png|svg)$": "<rootDir>/fileTransformer.js",
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "homepage": "https://github.com/oslabs-beta/irisql#readme",
